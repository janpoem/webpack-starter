{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "jsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "loose": false,
    "externalHelpers": true,
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "optimizer": {
        "simplify": true
      },
      "react": {
        "runtime": "automatic",
        "importSource": "react",
        "refresh": true
      }
    }
  },
  "env": {
    "targets": [
      "defaults"
    ],
    "mode": "entry",
    "coreJs": 3
  },
  "sourceMaps": true
}
